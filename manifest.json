{
    "manifest_version": 2,
    "name": "Heroes of the Storm Stream Browser",
    "version": "0.2",
    "description": "Heroes of the Storm (BlizzHeroes) Stream Browser",
    "permissions":["https://api.twitch.tv/"],
    "browser_action":{
        "default_title": "Heroes of the Storm Stream Browser",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
    "icons": {
           "48": "icon48.png",
          "128": "icon128.png" }
}
